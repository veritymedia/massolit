{
  "private": true,
  "scripts": {
    "dev": "pnpm generate && concurrently 'pnpm:dev:*'",
    "generate": "nuxt generate && cp -R .output ./pocketbase",
    "dev:nuxt": "nuxt dev",
    "dev:pocketbase": "go run pocketbase/main.go serve",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "build:prod": "nuxt generate && cp -R .output ./pocketbase && go build -o pocketnuxt pocketbase/main.go"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@vueuse/core": "^11.1.0",
    "@vueuse/nuxt": "^11.1.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^7.6.0",
    "nuxt": "3.13.2",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14"
  },
  "dependencies": {
    "@radix-icons/vue": "^1.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "html5-qrcode": "^2.3.8",
    "pocketbase": "^0.9.1",
    "radix-vue": "^1.9.7",
    "shadcn-nuxt": "^0.10.4",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.6.3"
  }
